{
  "compilerOptions": {
    "jsx": "react-jsx",
    "target": "es6",
    "module": "commonjs",
    "baseUrl": "./src",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "allowJs": true,
    "declaration": true,
    "declarationDir": "lib",
    "noImplicitAny": false,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": false,
    "strictFunctionTypes": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "importHelpers": true,
    "noEmitOnError": true,
    "lib": [
      "dom",
      "es2019"
    ],
    "typeRoots": [
      "node_modules/@types"
    ],
    "skipLibCheck": true
  }
}
